@model IEnumerable<ElaineCristina09112017.Empresa>


<nav aria-label="breadcrumb" role="navigation">
    <ol class="breadcrumb">
        <li class="breadcrumb-item active" aria-current="page">Empresas</li>
    </ol>
</nav>

<div class="row">
    <div class="col-md-2">
        @Html.ActionLink("Cadastrar Empresa", "Create", null, new { @class="btn btn-primary"})
    </div>
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.STRNOME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STRCNPJ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DTCADASTRO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STRRAZAOSOCIAL)
        </th>
        <th></th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.STRNOME)
        </td>
        <td id="tdCNPJ">
            @Html.DisplayFor(modelItem => item.STRCNPJ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DTCADASTRO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STRRAZAOSOCIAL)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IDEMPRESA }) |
            @Html.ActionLink("Visualizar", "Details", new { id=item.IDEMPRESA }) |
            @Html.ActionLink("Deletar", "Delete", new { id=item.IDEMPRESA })
        </td>
    </tr>
}
    
</table>


<script>
    $(document).ready(function () {

        $("#tdCNPJ").mask('00.000.000/0000-00');
    });
</script>